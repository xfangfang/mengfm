        {
            "dialogue_id": 428498921,
            "dialogue_content": "是吗？那我等着哦~",
            "dialogue_delay": 0,
            "dialogue_image": null,
            "dialogue_sound": null,
            "dialogue_sound_duration": 0,
            "dialogue_sound_vols": 0,
