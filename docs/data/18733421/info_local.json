            "phase_sound": "data/18733421/Y95HpUFRvo7mN018.mp3",
            "phase_sound_duration": 20402,
            "role_name": "沈卿",
            "phase_video_info": null
        },
        {
            "dialogue_id": 428498920,
            "dialogue_content": "最好打消你的念头，不然我会让你亲身感受一下成为标本的乐趣（冷漠）",
            "dialogue_delay": 0,
            "dialogue_image": null,
            "dialogue_sound": null,
            "dialogue_sound_duration": 0,
            "dialogue_sound_vols": 0,
