            "phase_sound_duration": 10893,
            "role_name": "赌徒",
            "phase_video_info": null
        },
        {
            "dialogue_id": 541200548,
            "dialogue_content": "（轻笑）未成年的小朋友，来这种地方好像不太妥当。",
            "dialogue_delay": 0,
            "dialogue_image": null,
            "dialogue_sound": null,
            "dialogue_sound_duration": 0,
            "dialogue_sound_vols": 0,
            "dialogue_bgm": null,
            "dialogue_bgm_duration": 0,
            "dialogue_bgm_vols": 0,
            "dialogue_index": 6,
            "role_id": 1470450,
            "dialogue_sound_name": null,
            "dialogue_bgm_name": null,
            "dialogue_reverb": 0,
            "dialogue_compose": {
                "copyInt": "1"
            },
            "dialogue_video": null,
            "dialogue_video_duration": 0,
            "dialogue_video_timer": null,
            "phase_bgm": null,
