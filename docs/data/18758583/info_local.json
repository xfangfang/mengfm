            "phase_sound_duration": 16744,
            "role_name": "庄主",
            "phase_video_info": null
        },
        {
            "dialogue_id": 541200554,
            "dialogue_content": "无语速缓慢，不疾不徐的将烟灰弹落。",
            "dialogue_delay": 5000,
            "dialogue_image": null,
            "dialogue_sound": null,
            "dialogue_sound_duration": 0,
            "dialogue_sound_vols": 0,
            "dialogue_bgm": null,
