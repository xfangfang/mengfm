            "phase_sound": "data/18758583/GxW73Mdvb5lwqfOr.mp3",
            "phase_sound_duration": 7367,
            "role_name": "庄主",
            "phase_video_info": null
        },
        {
            "dialogue_id": 541200549,
            "dialogue_content": "不巧，我成年了。（挑衅）听传闻说吴先生赌单双最为出色，那就玩单双，怎么样?",
            "dialogue_delay": 0,
            "dialogue_image": null,
            "dialogue_sound": null,
            "dialogue_sound_duration": 0,
            "dialogue_sound_vols": 0,
            "dialogue_bgm": null,
            "dialogue_bgm_duration": 0,
            "dialogue_bgm_vols": 0,
